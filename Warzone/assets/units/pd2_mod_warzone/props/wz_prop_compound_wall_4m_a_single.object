<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_warzone/props/wz_prop_compound_wall" orientation_object="rp_root" />
	
	<bodies>
		<body name="body_main" enabled="true" template="static">
			<object name="rp_root"/>
			<object name="c_box_001"	collision_type="box" padding="-2.5"/>
		</body>	
	</bodies>
	
	<decal_surfaces default_material="concrete" static="true">
        <decal_mesh enabled="true" name="dm_dm" material="concrete" />
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<lod_object name="lod_group">
				<object name="g_wall_lod0"	lod="0" enabled="true" shadow_caster="true" max_distance="12800" />
				<object name="g_wall_lod1"	lod="1" enabled="true" shadow_caster="true" max_distance="25600" />
				<object name="g_wall_lod2"	lod="2" enabled="true" shadow_caster="true" max_distance="51200" />
				<object name="g_wall_lod3" 	lod="3" enabled="true" shadow_caster="false" />
			</lod_object>
		</graphic_group>
	</graphics>
</dynamic_object>