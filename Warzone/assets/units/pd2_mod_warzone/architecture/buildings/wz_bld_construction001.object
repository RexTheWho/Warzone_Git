<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_warzone/architecture/buildings/wz_bld_construction001" orientation_object="rp_root" />
	
	<bodies>
		<body name="body_main" enabled="true" template="static">
			<object name="rp_root"/>
			<object name="g_main"		collision_type="mesh_mopp" padding="-2.5" />
		</body>	
		<body name="body_mover_blocker" enabled="true" template="mover_blocker">
			<object name="g_alpha"		collision_type="mesh_mopp" padding="-2.5" />
		</body>	
	</bodies>
	
	<decal_surfaces default_material="metal" static="true">
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<object name="g_main"		enabled="true" 	shadow_caster="true"/>
			<object name="g_alpha"		enabled="true" 	shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>